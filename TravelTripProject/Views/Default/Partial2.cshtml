
@using TravelTripProject.Models.Siniflar
@model List<Blog>
@foreach (var x in Model)
{


    <div class="right">
        <div class="bot-left">
            <img src="@x.BlogImage" alt="" />
        </div>
        <div class="left-text">

            <h4>@x.Baslik</h4>
            <span class="doller">@x.Tarih</span>
            <p>açıklamalı</p>
            <p><a href="/Blog/BlogDetay/@x.ID"><span class="read">Devamını Oku >></span></a></p>
        </div>
    </div>
}


